{
    "manifest_version": 3,
    "name": "Focus Tab",
    "version": "0.0.1",
    "description": "__MSG_appDescription__",
    "default_locale": "en", 
    "author": "Roberto Iacono",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "action": {
      "default_popup": "newtab.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
  }
  